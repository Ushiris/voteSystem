{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "allowJs": true,
    "checkJs": true,
    "target": "es2015",
    "strict": true,
    "moduleResolution": "Node",
    "jsx": "react",
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
      "src/**/*"
  ]
}